{
   "author": "RAZORlabs",
   "background": {
      "scripts": [ "js/background/background.js" ]
   },
   "browser_action": {
      "default_icon": "res/icon/48.png",
      "default_popup": "html/browser-action/home.html",
      "default_title": "Night Eye"
   },
   "commands": {
      "toggle-current-website": {
         "description": "__MSG_extToggleButton__",
         "suggested_key": {
            "default": "Alt+Shift+2",
            "mac": "Command+Shift+2"
         }
      },
      "toggle-power-extension": {
         "description": "Toggle on and off the extension",
         "suggested_key": {
            "default": "Alt+Shift+3",
            "mac": "Command+Shift+3"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/loading.css" ],
      "js": [ "js/content/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'sha256-iFQqEQOE+73K+zzD6AOo8l3XTc/xh9rAqvi2Cigd2pQ=' 'sha256-uy8D2ip29CkJtS9XwyNKoaLIiSY8XL2rfa9TwKhhK4o='; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "homepage_url": "https://nighteye.app/",
   "icons": {
      "128": "res/icon/128.png",
      "16": "res/icon/16.png",
      "48": "res/icon/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "offline_enabled": true,
   "permissions": [ "contextMenus", "activeTab", "tabs", "webRequest", "webRequestBlocking", "*://*/*" ],
   "short_name": "Night Eye",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.5.4",
   "web_accessible_resources": [ "images/*", "css/*", "_locales/*", "js/browser-action/*" ]
}
