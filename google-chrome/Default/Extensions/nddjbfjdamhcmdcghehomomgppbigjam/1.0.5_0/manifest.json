{
   "browser_action": {
      "default_popup": "/views/popup.html"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Gentle reminder about how fleeting life is. Time to make the most of it!",
   "icons": {
      "128": "icons/hourglass-128.png",
      "16": "icons/hourglass-16.png",
      "48": "icons/hourglass-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Death Clock",
   "permissions": [ "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5"
}
